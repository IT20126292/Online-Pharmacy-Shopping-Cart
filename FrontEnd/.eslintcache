;  \r\n  })\r\n\r\n  tickets.map(tickettot => {\r\n\r\n    const total = [       \r\n    tickettot.totalPrice,       \r\n\r\n    ];\r\n    tableRows.push(total);\r\n  })\r\n\r\n  doc.text(\"CITY MEDICALS\", 70, 8).setFontSize(13);\r\n  doc.text(\"Cart Invoice\", 14, 16).setFontSize(13);\r\n  doc.text(`Date - ${dateStr}`, 14, 23);\r\n\r\n  //right down width height\r\n  //doc.addImage(img, 'JPEG', 170, 8, 25, 25);\r\n\r\n  doc.autoTable(tableColumn, tableRows, { styles: { fontSize: 8, }, startY:35});\r\n  doc.save(\"Product Report.pdf\");\r\n\r\n  };\r\n\r\n  return (\r\n    <>\r\n      <div className=\"cartscreen\">\r\n        <div className=\"cartscreen__left\">\r\n          <h2>My Cart</h2>\r\n\r\n          {cartItems.length === 0 ? (\r\n            <div>\r\n              Your Cart Is Empty <Link to=\"/\">Go Back</Link>\r\n            </div>\r\n          ) : (\r\n            cartItems.map((item) => (\r\n              <CartItem\r\n                key={item.product}\r\n                item={item}\r\n                qtyChangeHandler={qtyChangeHandler}\r\n                removeHandler={removeFromCartHandler}\r\n              />\r\n            ))\r\n          )}\r\n        </div>\r\n\r\n        <div className=\"cartscreen__right\">\r\n          <div className=\"cartscreen__info\">\r\n            <p>Sub Total ({getCartCount()}):   Rs.{getCartSubTotal()}</p>\r\n            <p style={{ fontSize: 15, color: '#ff8605' }}>Discount:  Rs.0.00</p>\r\n            <br/>\r\n            <p style={{ fontSize: 22, color: '#00da7e' }}><strong>Total Price:  Rs.{getCartSubTotal()}</strong></p>\r\n          </div>\r\n          <div>\r\n            <button>Checkout</button>\r\n          </div>\r\n          {/* pdf generate */}\r\n          <div class=\"buttonn\">\r\n            <button type=\"button\" class=\"btn btn-primary\" style={{backgroundColor:'#133C48',width:'380px'}} onClick={() => generatePDF(cartItems)} >GenerateReport</button> <br></br>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default CartScreen;\r\n","E:\\ITPM-Project-2022\\New folder\\ITPM-Project\\FrontEnd\\src\\screens\\ProductScreen.js",[],"E:\\ITPM-Project-2022\\New folder\\ITPM-Project\\FrontEnd\\src\\redux\\reducers\\productReducers.js",[],"E:\\ITPM-Project-2022\\New folder\\ITPM-Project\\FrontEnd\\src\\redux\\reducers\\cartReducers.js",[],"E:\\ITPM-Project-2022\\New folder\\ITPM-Project\\FrontEnd\\src\\components\\shopping-cart\\Backdrop.js",[],"E:\\ITPM-Project-2022\\New folder\\ITPM-Project\\FrontEnd\\src\\components\\shopping-cart\\SideDrawer.js",[],"E:\\ITPM-Project-2022\\New folder\\ITPM-Project\\FrontEnd\\src\\screens\\HomeScreen.js",[],"E:\\ITPM-Project-2022\\New folder\\ITPM-Project\\FrontEnd\\src\\components\\header\\header.js",[],"E:\\ITPM-Project-2022\\New folder\\ITPM-Project\\FrontEnd\\src\\components\\footer\\footer.js",[],"E:\\ITPM-Project-2022\\New folder\\ITPM-Project\\FrontEnd\\src\\components\\shopping-cart\\CartItem.js",[],"E:\\ITPM-Project-2022\\New folder\\ITPM-Project\\FrontEnd\\src\\components\\shopping-cart\\Product.js",[],"E:\\ITPM-Project-2022\\New folder\\ITPM-Project\\FrontEnd\\src\\redux\\constants\\cartConstants.js",[],"E:\\ITPM-Project-2022\\New folder\\ITPM-Project\\FrontEnd\\src\\redux\\constants\\productConstants.js",[],"E:\\ITPM-Project-2022\\New folder\\ITPM-Project\\FrontEnd\\src\\redux\\actions\\cartActions.js",[],"E:\\ITPM-Project-2022\\New folder\\ITPM-Project\\FrontEnd\\src\\redux\\actions\\productActions.js",[],{"ruleId":"109","replacedBy":"110"},{"ruleId":"111","replacedBy":"112"},{"ruleId":"109","replacedBy":"113"},{"ruleId":"111","replacedBy":"114"},{"ruleId":"115","severity":1,"message":"116","line":50,"column":22,"nodeType":"117","messageId":"118","endLine":50,"endColumn":24},{"ruleId":"115","severity":1,"message":"116","line":62,"column":25,"nodeType":"117","messageId":"118","endLine":62,"endColumn":27},"no-native-reassign",["119"],"no-negated-in-lhs",["120"],["119"],["120"],"arr